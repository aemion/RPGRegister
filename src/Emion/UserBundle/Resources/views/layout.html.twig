{% extends "::layout.html.twig" %}

{% block body %}
  
  {% if app.request.hasPreviousSession %}
    {% for key, message in app.session.flashbag.all() %}
      <div class="alert alert-{{ key }}">
        {{ message|trans({}, 'FOSUserBundle') }}
      </div>
    {% endfor %}
  {% endif %}
  
  
  {% block fos_user_content %}
  {% endblock fos_user_content %}

{% endblock %}